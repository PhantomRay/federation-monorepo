{
  "name": "@incognito/auth",
  "version": "2.0.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "prebuild": "pnpm clean",
    "clean": "rimraf .turbo && rimraf node_modules && rimraf dist",
    "build": "nest build",
    "postbuild": "pnpm copy",
    "start": "nest start",
    "dev": "PORT=3001 nest start --watch",
    "start:prod": "node dist/main",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "NODE_OPTIONS=--no-experimental-fetch jest --runInBand --detectOpenHandles --forceExit --verbose",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest.config.js --forceExit --runInBand --verbose",
    "migrate:dev": "dotenv -e .env.local prisma migrate dev --preview-feature",
    "migrate:dev:create": "dotenv -e .env.local prisma migrate dev --create-only --preview-feature",
    "migrate:reset": "dotenv -e .env.local prisma migrate reset --preview-feature",
    "migrate:deploy": "dotenv -e .env.local prisma migrate deploy --preview-feature",
    "migrate:status": "dotenv -e .env.local prisma migrate status --preview-feature",
    "migrate:resolve": "dotenv -e .env.local prisma migrate resolve --preview-feature",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "db:push": "prisma db push --preview-feature --skip-generate",
    "db:push:test": "dotenv -e .env.test prisma db push --preview-feature --skip-generate",
    "copy": "pnpm copy:prisma && pnpm copy:gql",
    "copy:prisma": "copyfiles -u 1 -a -E \"src/.prisma/**/*\" dist",
    "copy:gql": "copyfiles -u 1 -a src/**/*.gql src/**/**/*.gql dist"
  },
  "dependencies": {
    "@nestjs/common": "10.0.5",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "10.0.5",
    "@nestjs/jwt": "10.1.0",
    "@nestjs/microservices": "10.0.5",
    "@nestjs/passport": "10.0.0",
    "@nestjs/platform-express": "10.0.5",
    "@nestjs/swagger": "7.1.1",
    "@prisma/client": "5.0.0",
    "bowser": "2.11.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "dayjs": "1.11.9",
    "dotenv": "16.3.1",
    "ioredis": "5.3.2",
    "joi": "17.9.2",
    "lodash": "4.17.21",
    "ms": "2.1.3",
    "nestjs-prisma": "0.21.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "superagent": "8.0.9",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.1.8",
    "@nestjs/schematics": "10.0.1",
    "@nestjs/testing": "10.0.5",
    "@types/supertest": "2.0.12",
    "copyfiles": "2.4.1",
    "prisma": "5.0.0",
    "supertest": "6.3.3"
  }
}
